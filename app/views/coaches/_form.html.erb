<%= simple_form_for [@gymnasia, @coach] do |f| %>

  <div class="group">
    <%= f.input :name %>
  </div>

  <div class="group">
    <%= f.input :major %>
  </div>

  <div class="group">
    <%= f.input :introduction %>
  </div>

  <%= f.submit "Submit", data: { disable_with: "Submitting..." } %>

<% end %>
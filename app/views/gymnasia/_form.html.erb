<%= simple_form_for @gymnasia do |f| %>

  <div class="group">
    <%= f.input :name %>
  </div>

  <div class="group">
    <%= f.input :phone %>
  </div>

  <div class="group">
    <%= f.input :address %>
  </div>

  <div class="group">
    <%= f.input :introduction %>
  </div>

  <%= f.submit "Submit", data: { disable_with: "Submitting..." } %>

<% end %>